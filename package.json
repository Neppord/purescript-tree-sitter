{
  "name": "purescript-tree-sitter",
  "version": "1.0.0",
  "description": "Purescript bindings for tree-sitter",
  "main": "index.js",
  "scripts": {
    "spago": "spago",
    "pulp": "pulp",
    "start": "spago run",
    "publish": "pulp publish",
    "publish:patch": "spago bump-version patch --no-dry-run && pulp publish",
    "examples": "spago test --main Examples.Main",
    "test": "spago test",
    "repl": "spago repl",
    "purs-tidy": "purs-tidy",
    "format": "purs-tidy format-in-place src test examples"
  },
  "keywords": [
    "parser",
    "lexer",
    "tree-sitter"
  ],
  "author": "Samuel Ytterbrink",
  "license": "MIT",
  "dependencies": {
    "bower": "^1.8.13",
    "mocha": "^9.2.1",
    "purescript": "^0.14.4",
    "spago": "^0.20.7",
    "tree-sitter": "^0.20.0"
  },
  "devDependencies": {
    "pulp": "^15.0.0",
    "purs-tidy": "^0.7.0",
    "tree-sitter-bash": "^0.19.0",
    "tree-sitter-swift": "npm:experimental-tree-sitter-swift@^0.0.2"
  }
}
